{
  "name": "Binance Spot Signal Bot",
  "version": "0.1.1",
  "slug": "binance_signal_bot",
  "description": "Binance spot scanner (USDT pairs) with Telegram signals + nice Web UI and native labeled options.",
  "arch": [
    "aarch64",
    "amd64"
  ],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "options": {
    "telegram_token": "",
    "telegram_chat_id": "",
    "timezone": "Asia/Seoul",
    "min_confirms": 3,
    "ema_fast": 20,
    "ema_mid": 50,
    "ema_slow": 200,
    "rsi_length": 14,
    "macd_fast": 12,
    "macd_slow": 26,
    "macd_signal": 9,
    "rvol15m_min": 1.6,
    "bias_rsi_min": 50,
    "bias_need_ema_order": true,
    "bias_allow_price_above_ema200_15m": true,
    "breakout_lookback_bars": 10,
    "macd_hist_rising_bars_min": 2,
    "macd_cross_up_allowed": true,
    "breakout_body_max_atr_mult": 1.8,
    "ema200_5m_min_distance_pct": 0.2,
    "taker_fee_bps": 10,
    "roundtrip_extra_buffer_bps": 5,
    "min_net_profit_bps": 10,
    "tp_levels_pct": [
      0.007,
      0.012,
      0.02
    ],
    "use_level1_spread": false
  },
  "schema": {
    "telegram_token": "str",
    "telegram_chat_id": "str",
    "min_confirms": "int",
    "timezone": "str"
  },
  "ingress": false,
  "webui": "http://[HOST]:[PORT:8080]",
  "ports": {
    "8080/tcp": 8080
  },
  "map": [
    "share:rw",
    "data:rw"
  ],
  "privileged": []
}