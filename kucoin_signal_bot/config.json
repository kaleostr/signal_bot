{
  "name": "KuCoin Spot Signal Bot",
  "version": "0.2.4",
  "slug": "kucoin_signal_bot",
  "description": "KuCoin spot scanner (USDT pairs) with Telegram signals + nice Web UI and native labeled options.",
  "arch": [
    "aarch64",
    "amd64"
  ],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "options": {
    "telegram_token": "",
    "telegram_chat_id": "",
    "symbols_quote": "USDT",
    "top_n_by_volume": 120,
    "min_vol_24h_usd": 5000000,
    "cooldown_minutes": 20,
    "timezone": "Asia/Seoul",
    "use_level1_spread": false,
    "min_confirms": 3,
    "ema_fast": 20,
    "ema_mid": 50,
    "ema_slow": 200,
    "rsi_length": 14,
    "macd_fast": 12,
    "macd_slow": 26,
    "macd_signal": 9,
    "rvol15m_min": 1.6,
    "bias_rsi_min": 50,
    "bias_need_ema_order": true,
    "bias_allow_price_above_ema200_15m": true,
    "breakout_lookback_bars": 3,
    "macd_hist_rising_bars_min": 1,
    "macd_cross_up_allowed": true,
    "breakout_body_max_atr_mult": 1.8,
    "ema200_5m_min_distance_pct": 0.2,
    "taker_fee_bps": 10,
    "roundtrip_extra_buffer_bps": 5,
    "min_net_profit_bps": 0,
    "tp_levels_pct": "0.007, 0.012, 0.020",
    "bias_ema_order_mode": "fast_over_mid",
    "bias_macd_condition": "hist_rising"
  },
  "schema": {
    "telegram_token": "str",
    "telegram_chat_id": "str",
    "symbols_quote": "str",
    "top_n_by_volume": "int",
    "min_vol_24h_usd": "int",
    "cooldown_minutes": "int",
    "timezone": "str",
    "use_level1_spread": "bool",
    "min_confirms": "int",
    "ema_fast": "int",
    "ema_mid": "int",
    "ema_slow": "int",
    "rsi_length": "int",
    "macd_fast": "int",
    "macd_slow": "int",
    "macd_signal": "int",
    "rvol15m_min": "float",
    "bias_rsi_min": "int",
    "bias_need_ema_order": "bool",
    "bias_allow_price_above_ema200_15m": "bool",
    "breakout_lookback_bars": "int",
    "macd_hist_rising_bars_min": "int",
    "macd_cross_up_allowed": "bool",
    "breakout_body_max_atr_mult": "float",
    "ema200_5m_min_distance_pct": "float",
    "taker_fee_bps": "int",
    "roundtrip_extra_buffer_bps": "int",
    "min_net_profit_bps": "int",
    "tp_levels_pct": "str",
    "bias_ema_order_mode": "str",
    "bias_macd_condition": "str"
  },
  "ingress": false,
  "webui": "http://[HOST]:[PORT:8080]",
  "ports": {
    "8080/tcp": 8080
  },
  "map": [
    "share:rw",
    "data:rw"
  ],
  "privileged": []
}