{
  "name": "KuCoin Spot Signal Bot",
  "version": "0.3.1",
  "slug": "kucoin_signal_bot",
  "description": "KuCoin spot scanner (USDT pairs) with Telegram alerts and dark Web UI. Bias 1h → Setup 15m → Trigger 5m. Per-timeframe indicators.",
  "arch": [
    "aarch64",
    "amd64"
  ],
  "startup": "services",
  "boot": "auto",
  "init": false,
  "options": {
    "telegram_token": "",
    "telegram_chat_id": "",
    "min_vol_24h_usd": 5000000,
    "cooldown_minutes": 20,
    "timezone": "Asia/Seoul",
    "symbols_quote": "USDT",
    "top_n_by_volume": 120,
    "min_confirms": 3,
    "ema_fast": 20,
    "ema_mid": 50,
    "ema_slow": 200,
    "rsi_length": 14,
    "macd_fast": 12,
    "macd_slow": 26,
    "macd_signal": 9,
    "rvol15m_min": 1.6,
    "breakout_body_max_atr_mult": 1.8,
    "ema200_5m_min_distance_pct": 0.2,
    "taker_fee_bps": 10,
    "roundtrip_extra_buffer_bps": 5,
    "min_net_profit_bps": 10,
    "tp_levels_pct": [
      0.007,
      0.012,
      0.02
    ],
    "use_level1_spread": false
  },
  "schema": {
    "telegram_token": "str",
    "telegram_chat_id": "str",
    "min_vol_24h_usd": "int",
    "cooldown_minutes": "int",
    "timezone": "str",
    "symbols_quote": "str",
    "top_n_by_volume": "int",
    "min_confirms": "int"
  },
  "ingress": false,
  "webui": "http://[HOST]:[PORT:8080]",
  "ports": {
    "8080/tcp": 8080
  },
  "map": [
    "share:rw",
    "data:rw"
  ],
  "privileged": []
}